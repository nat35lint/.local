#!/bin/bash

rm -rf ~/.config/qtile

cp -r ~/.local/backups/qtilecpn ~/.config/

cd ~/.config

mv qtilecpn qtile

cd

rm -rf ~/.config/kitty

cp -r ~/.local/backups/kittycpn ~/.config

cd ~/.config

mv kittycpn kitty

cd

rm -rf ~/.config/cava

cp -r ~/.local/backups/cavacpn ~/.config

cd ~/.config

mv cavacpn cava

cd
